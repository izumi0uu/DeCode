{
  "kind": "collectionType",
  "collectionName": "lessons",
  "info": {
    "singularName": "lesson",
    "pluralName": "lessons",
    "displayName": "Lesson",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "default": "Unnamed",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "description": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "duration": {
      "type": "integer"
    },
    "sortOrder": {
      "type": "integer"
    },
    "content": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "videoUrl": {
      "type": "string"
    },
    "isPreview": {
      "type": "boolean",
      "default": false
    },
    "course": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::course.course",
      "required": true
    },
    "quizzes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::quiz.quiz"
    },
    "resources": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::resource.resource"
    },
    "lessonPublishedAt": {
      "type": "datetime"
    }
  }
}
